	new Cache: result, rows;

	result = mysql_query(mysql, "SELECT * FROM entrances", true);
	rows = cache_num_rows();

	if(rows > MAX_ENTRANCES)
	{
		rows = MAX_ENTRANCES;
		print("[Entrances]: DB rows > MAX_ENTRANCES");
