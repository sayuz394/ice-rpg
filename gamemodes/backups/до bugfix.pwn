		return SendClientMessage(playerid, 0x999999FF, "Банды: 0-Свободная зона, 1-Grove Gang, 2-Vagos Gang, 3-Ballas Gang, 4-Aztecas Gang");

	new gang_zone_id = -1;

	for(new idx; idx < g_gang_zones_loaded; idx ++)
	{
		if(!IsPlayerInDynamicArea(playerid, GetGangZoneData(idx, GZ_AREA))) continue;

