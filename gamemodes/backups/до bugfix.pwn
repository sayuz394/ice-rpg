new MYSQL_HOST[32],
	MYSQL_USER[32],
	MYSQL_BASE[32],
	MYSQL_PASS[32];

stock LoadMySQLSettings()
{
	new file_id = ini_openFile("db_settings.ini"),
		error_id;

	if(file_id < 0)
	{
		printf("[MySQL Settings] Error while opening MySQL settings file. Error code: %d", file_id);
		return 0;
	}
	error_id = ini_getString(file_id, "host", MYSQL_HOST);
	
	if(error_id < 0) printf("Error while reading MySQL settings file (host). Error code: %d", error_id);
	
	error_id = ini_getString(file_id, "username", MYSQL_USER);
	
	if(error_id < 0) printf("Error while reading MySQL settings file (username). Error code: %d", error_id);
	
	error_id = ini_getString(file_id, "password", MYSQL_PASS);
	
	if(error_id < 0) printf("Error while reading MySQL settings file (password). Error code: %d", error_id);
	
	error_id = ini_getString(file_id, "database", MYSQL_BASE);
	
	if(error_id < 0) printf("Error while reading MySQL settings file (database). Error code: %d", error_id);
	
	ini_closeFile(file_id);
	
	return 1;
}

