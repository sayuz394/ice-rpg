stock GetNearestVehicleID(playerid, Float: radius = 0.0)
{
	if(radius == 0.0)
		radius = FLOAT_INFINITY;

	new vehicleid = INVALID_VEHICLE_ID;

	new Float: dist;
	new Float: pos_x, Float: pos_y, Float: pos_z;

	GetPlayerPos(playerid, pos_x, pos_y, pos_z);
	for(new idx; idx < MAX_VEHICLES; idx ++)
	{
		if(!IsValidVehicle(idx)) continue;
		if(!IsVehicleStreamedIn(idx, playerid)) continue;

		dist = GetVehicleDistanceFromPoint(idx, pos_x, pos_y, pos_z);
		if(dist < radius)
		{
			radius = dist,
			vehicleid = idx;
		}
	}
	return vehicleid;
}

stock GetNearestTruckTerminal(playerid, Float: range)
{
	new Float: x,
	    Float: y,
	    Float: z,
	    Float: min_dist = 9999.9,
		tt_id = -1; // id терминала

	GetPlayerPos(playerid, x, y, z);

	for(new idx; idx < sizeof truck_terminal; idx ++)
	{
	    new Float: tt_distance = GetDistanceBetweenPoints(x, y, z, truck_terminal[idx][TT_LOAD_X], truck_terminal[idx][TT_LOAD_Y], truck_terminal[idx][TT_LOAD_Z]);

	    if(min_dist > tt_distance)
	    {
	        min_dist = tt_distance;

	        if(tt_distance <= range) tt_id = idx;
	    }
	}

	return tt_id;
}

stock GetNearestTruckContract(playerid, Float: range)
{
	new Float: x,
	    Float: y,
	    Float: z,
	    Float: min_dist = 9999.9,
		tc_id = -1; // id контракта

	GetPlayerPos(playerid, x, y, z);

	for(new idx; idx < sizeof truck_contract; idx ++)
	{
	    new Float: tc_distance = GetDistanceBetweenPoints(x, y, z, truck_contract[idx][TC_LOAD_X], truck_contract[idx][TC_LOAD_Y], truck_contract[idx][TC_LOAD_Z]);

	    if(min_dist > tc_distance)
	    {
	        min_dist = tc_distance;

	        if(tc_distance <= range) tc_id = idx;
	    }
	}

	return tc_id;
}

