					SendClientMessage(playerid, 0xffff00FF, fmt_str);
				}

				if(pay_sum > 0)
					GivePlayerMoneyEx(playerid, pay_sum, "Gaji di tempat kerja sebagai loader", true, false);

				format(fmt_str, sizeof fmt_str, "~b~~h~+$%d", pay_sum);
				GameTextForPlayer(playerid, fmt_str, 4000, 1);
			}
			case TEMP_JOB_LOADERA:
			{
				DisablePlayerCheckpoint(playerid);
				DisablePlayerRaceCheckpoint(playerid);

				TogglePlayerDynamicCP(playerid, help_info_CP, true);

				if(annul)
				    SendClientMessage(playerid, 0xCECECEFF, "Anda meninggalkan gudang.");

				new ts_pay = false;

				pay_sum = items * (2 * GetPlayerData(playerid, P_LOAD_SKILL) + GetTempJobInfo(job, TJ_PAY_FOR_LOAD));

				if(pay_sum > 0)
					format(fmt_str, sizeof fmt_str, ". Berpenghasilan {FFFFFF}$%d", pay_sum);

				strins(fmt_str, "Hari kerja sudah berakhir", 0, sizeof fmt_str);
				SendClientMessage(playerid, 0xffff00FF, fmt_str);

				if(ts_pay)
				{
					format(fmt_str, sizeof fmt_str, "{FFFFFF}$%d {ffff00}untuk bekerja di forklift", pay_sum);
					SendClientMessage(playerid, 0xffff00FF, fmt_str);
				}

				if(pay_sum > 0)
					GivePlayerMoneyEx(playerid, pay_sum, "Gaji di tempat kerja sebagai loader", true, false);

				format(fmt_str, sizeof fmt_str, "~b~~h~+$%d", pay_sum);
				GameTextForPlayer(playerid, fmt_str, 4000, 1);
			}
			case TEMP_JOB_MINER:
			{
				DisablePlayerCheckpoint(playerid);

				if(!annul)
				{
					if(items > 0)
					{
						format(fmt_str, sizeof fmt_str, "Hari kerja berakhir. Total kg diekstraksi. bijih: {FFFFFF}%d", items);
						SendClientMessage(playerid, 0xffff00FF, fmt_str);

						format(fmt_str, sizeof fmt_str, "Total yang diterima {FFFFFF}$%d", pay_sum);
						SendClientMessage(playerid, 0xffff00FF, fmt_str);

						GivePlayerMoneyEx(playerid, pay_sum, "Gaji penambang", true, false);

						format(fmt_str, sizeof fmt_str, "~b~~h~+$%d", pay_sum);
						GameTextForPlayer(playerid, fmt_str, 4000, 1);
					}
					else SendClientMessage(playerid, 0xffff00FF, "Hari kerja sudah berakhir");
				}
				else SendClientMessage(playerid, 0xCECECEFF, "Anda meninggalkan tambang. Penghasilan telah dibatalkan");

				ApplyAnimationEx(playerid, "CARRY", "crry_prtial", 4.1, 0, 0, 0, 0, 1, 1, USE_ANIM_TYPE_NONE);
			}
			case TEMP_JOB_PIZZA:
			{
				DisablePlayerCheckpoint(playerid);
				if(items > 0)
				{
					format(fmt_str, sizeof fmt_str, "Hari kerja berakhir. Total terkirim: {FFFFFF}%d", items);
