CMD:dice(playerid, params[])
{
	if(GetPlayerInBiz(playerid) == -1)
		return SendClientMessage(playerid, 0xCECECEFF, "Anda harus berada di kasino");

	if(GetBusinessData(GetPlayerInBiz(playerid), B_TYPE) != BUSINESS_TYPE_CASINO)
		return SendClientMessage(playerid, 0xCECECEFF, "Anda harus berada di kasino");

	extract params -> new to_player, money; else return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /dice [id pemain] [jumlahnya]");

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player) || to_player == playerid)
		return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");

	if(!IsPlayerInRangeOfPlayer(playerid, to_player, 4.0))
		return SendClientMessage(playerid, 0xCECECEFF, "Pemainnya terlalu jauh");

	if(!(1 <= money <= 1_000_000))
		return SendClientMessage(playerid, 0xCECECEFF, "Jumlah harus tidak lebih dari 1.000.000$");

	if(GetPlayerMoneyEx(playerid) < money)
		return SendClientMessage(playerid, 0xCECECEFF, "Anda tidak punya uang sebanyak itu");

	if(GetPlayerMoneyEx(to_player) < money)
		return SendClientMessage(playerid, 0xCECECEFF, "Pemain tidak memiliki uang sebanyak itu");

	SendPlayerOffer(playerid, to_player, OFFER_TYPE_CASINO_DICE, money);

	return 1;
}

CMD:ad(playerid, params[])
{
	if(!GetPlayerPhone(playerid))
		return SendClientMessage(playerid, 0x999999FF, "Anda tidak memiliki telepon untuk menghubungi Anda");

	new fmt_text[144];

	for(new idx; idx < sizeof g_advert; idx ++)
	{
		if(GetAdvertData(idx, A_CREATOR) != playerid) continue;

		format(fmt_text, sizeof fmt_text, "Iklan Anda sudah sejalan di bawah {00CC99}angka %d", GetAdvertIndexInQueue(idx));
		SendClientMessage(playerid, 0xCECECEFF, fmt_text);

		return 1;
	}

	if(!strlen(params))
		return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /ad [teks iklan]");

	if(GetPlayerMoneyEx(playerid) < ADVERT_SEND_PRICE)
		return SendClientMessage(playerid, 0x999999FF, "Anda harus mengirim pengumuman 250$");

	if(SendAdvert(playerid, params))
	{
		GivePlayerMoneyEx(playerid, -250, "Kirim Iklan", true, true);

		format(fmt_text, sizeof fmt_text, "Teks: %s", params);
		SendClientMessage(playerid, 0x00CC99FF, fmt_text);

		SendClientMessage(playerid, -1, "Iklan Anda telah ditambahkan ke antrian untuk publikasi.");
		SendClientMessage(playerid, -1, "Tetap terhubung, jika tidak, iklan akan dihapus dari antrian");
	}
	else SendClientMessage(playerid, 0xFF5533FF, "Terjadi kesalahan saat mengirim iklan Anda. Hubungi administrasi");

	return 1;
}

