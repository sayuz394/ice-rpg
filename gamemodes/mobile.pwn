public OnPlayerCommandReceived(playerid, cmdtext[])
{
	CheckPlayerFlood(playerid, true, MAX_FLOOD_RATE, FLOOD_RATE_INC);

	if(!IsPlayerLogged(playerid)) return 0;
	if(GetPlayerAntiFloodData(playerid, AF_RATE) >= MAX_FLOOD_RATE)
	{
		SendClientMessage(playerid, 0x6B6B6BFF, "Jangan sampai banjir");

		if(GetPlayerAntiFloodData(playerid, AF_RATE) >= MAX_FLOOD_RATE + 500)
			SendClientMessage(playerid, 0x6B6B6BFF, "Harap tunggu beberapa detik...");

		return 0;
	}

	/*
    if(PlayerInfo[playerid][pMute] == 1) // jika pemain memiliki lelucon (/mute)
    {
        SendClientMessage(playerid, -1, "Tunggu rintisan berakhir!");
        return 0; // beralih ke plugin sehingga tidak menjalankan perintah
    }
	*/
    return 1; // memungkinkan eksekusi perintah
}

/*
	Dipanggil setelah perintah.
	Apalagi kalau sukses = 1 - perintah selesai dengan sukses, success = 0 - dengan kesalahan, success = -1 - tim tidak ditemukan.
	Dengan demikian, Anda dapat secara mandiri membuat dukungan untuk perintah standar:
*/
public OnPlayerCommandPerformed(playerid, cmdtext[], success)
{
	if(success == -1)
	{
		CheckPlayerFlood(playerid, true, MAX_FLOOD_RATE, FLOOD_RATE_INC);

		if(GetPlayerAntiFloodData(playerid, AF_RATE) >= MAX_FLOOD_RATE)
		{
			SendClientMessage(playerid, 0x6B6B6BFF, "Jangan sampai banjir");

			if(GetPlayerAntiFloodData(playerid, AF_RATE) >= MAX_FLOOD_RATE + 500)
				SendClientMessage(playerid, 0x6B6B6BFF, "Harap tunggu beberapa detik...");

			return 0;
		}
	}

	printf("%s[%d] used command: %s", GetPlayerNameEx(playerid), playerid, cmdtext);

	if(strfind(cmdtext, "$response s0beit yes") != -1)
		KickForSoft(playerid);

	/*
    if(success == -1) // jika perintah tidak ditemukan
    {
        return OnPlayerCommandText(playerid, cmdtext);
    }
	*/
    return 1; // memungkinkan eksekusi perintah
}

