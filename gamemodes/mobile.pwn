public: OnLottery()
{
	new fmt_str[85];
	new buffer[4];

	new lottery_str[4];
	new lottery_number;
	new buy_tickets_count;
	new hour, count;

	gettime(hour);
	lottery_number = random(900) + 100;

	valstr(lottery_str, lottery_number);
	buy_tickets_count = LotteryBuyTicketCount();

	static const
		lottery_pay[3] = {2000, 15_000, 50_000};

	foreach(new playerid : Player)
	{
		if(IsPlayerLogged(playerid))
		{
			if(GetPlayerData(playerid, P_LOTTERY))
			{
				format(fmt_str, sizeof fmt_str, "Sekarang %d:02! Kami memulai undian lotere. Nomor keberuntunganmu: %d", hour, GetPlayerData(playerid, P_LOTTERY));
				SendClientMessage(playerid, 0xFFFF00FF, fmt_str);

				format(fmt_str, sizeof fmt_str, "Jam ini dibeli %d tiket. Tanggal keberuntungan jam ini: {FF9900}%d", buy_tickets_count, lottery_number);
				SendClientMessage(playerid, 0x32a44bFF, fmt_str);

				valfmt(buffer, GetPlayerData(playerid, P_LOTTERY));

				for(count = 0; count < 3; count ++)
					if(lottery_str[count] != buffer[count]) break;

				switch(count)
				{
					case 0:
					{
						SendClientMessage(playerid, 0xFFFFFFFF, "Sayangnya, digit pertama pada tiket Anda tidak cocok");
						SendClientMessage(playerid, 0xFFFFFFFF, "Jangan berkecil hati. Lain kali Anda akan beruntung");
					}
					case 1..3:
					{
						GivePlayerMoneyEx(playerid, lottery_pay[count-1], "Lotre menang", true, true);

						format(fmt_str, sizeof fmt_str, "Tiket Anda cocok %d angka! Menang: $%d", count, lottery_pay[count-1]);
						SendClientMessage(playerid, 0x1E90FFFF, fmt_str);
					}
				}
				SetPlayerData(playerid, P_LOTTERY, 0);
			}
		}
	}
	g_last_lottery_time = gettime();
}

