				name,
				is_reg ? "Reg" : "Last",
				IsPlayerConnected(GetPlayerID(name)) ? "{66CC33}[online]" : "{888888}[offline]"
			);

			strcat(fmt_text, query);
		}

		format(query, sizeof query, "Akun ditemukan: %d / %d", l_acc_count, rows);

		Dialog(playerid, INVALID_DIALOG_ID, DIALOG_STYLE_MSGBOX, query, fmt_text, "Tutup", "");
	}
	else SendClientMessage(playerid, 0xCECECEFF, "Alamat IP ini tidak ditemukan dalam database.");

	cache_delete(result);

	return 1;
