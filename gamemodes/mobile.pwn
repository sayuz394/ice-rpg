CMD:sellmyhome(playerid, params[])
{
	new houseid = GetPlayerHouse(playerid, HOUSE_TYPE_HOME);
	if(houseid != -1)
	{
		if(!strlen(params))
			return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /sellmyhome [id pemain] [biaya]");

		extract params -> new to_player, price;

		new house_price = GetHouseData(houseid, H_PRICE);

		if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player) || to_player == playerid)
			return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");

		if(GetPlayerHouse(to_player) != -1)
			return SendClientMessage(playerid, 0xCECECEFF, "Pemain ini sudah memiliki rumah.");

		if(!((house_price / 3) <= price <= (house_price * 3)))
			return SendClientMessage(playerid, 0xCECECEFF, "Biaya tidak boleh lebih mahal atau lebih murah 3 kali dari biaya rumah");

		if(GetPlayerMoneyEx(to_player) < price)
			return SendClientMessage(playerid, 0xCECECEFF, "Pemain ini tidak memiliki jumlah itu.");

		if(!(IsPlayerInRangeOfHouse(playerid, houseid, 10.0) && IsPlayerInRangeOfHouse(to_player, houseid, 10.0)))
			return SendClientMessage(playerid, 0xCECECEFF, "Anda dan pembeli harus berada di dekat rumah yang ingin Anda jual.");

		SendPlayerOffer(playerid, to_player, OFFER_TYPE_SELL_HOME, houseid, price);
	}
	else SendClientMessage(playerid, 0xCECECEFF, "Anda tidak punya rumah");

	return 1;
}



