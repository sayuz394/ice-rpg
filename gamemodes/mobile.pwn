CMD:id(playerid, params[])
{
	if(!strlen(params))
		return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /id [nama pemain atau bagian]");

	new fmt_str[64];
	new count;

	foreach(new idx : Player)
	{
		if(!IsPlayerLogged(idx)) continue;

		if(strfind(GetPlayerNameEx(idx), params, true) != -1)
		{
			count ++;

			format(fmt_str, sizeof fmt_str, "%d. %s {32a44b}id %d", count, GetPlayerNameEx(idx), idx);
			SendClientMessage(playerid, 0xFFFFFFFF, fmt_str);

			if(count >= 5)
			{
				SendClientMessage(playerid, 0xCECECEFF, "Menampilkan 5 pertandingan pertama");
				break;
			}
		}
	}
	if(!count)
		SendClientMessage(playerid, 0xCECECEFF, "Tidak ditemukan kecocokan");

	return 1;
}

CMD:buyf(playerid, params[])
{
	new factory_fuels = GetRepositoryData(REPOSITORY_TYPE_OIL_FACTORY, REPOSITORY_ACTION_OIL_FACTORY_F, R_AMOUNT);

	if(!IsPlayerInAnyVehicle(playerid))
		return SendClientMessage(playerid, 0xCECECEFF, "Anda harus berada di kilang dengan kendaraan yang berfungsi");

	new modelid;
	new vehicleid = GetPlayerJobCar(playerid);

	if(vehicleid != INVALID_VEHICLE_ID)
		modelid = GetVehicleData(vehicleid, V_MODELID);

	if(IsPlayerInRangeOfPoint(playerid, 20.0, 994.1306, 679.8422, 12.0653))  // untuk pabrik
	{
		if(GetPlayerTempJob(playerid) != TEMP_JOB_FACTORY_TRUCKER || modelid != 514)
			return SendClientMessage(playerid, 0xCECECEFF, "Anda tidak bekerja di departemen pengiriman pabrik atau mengambil jalur yang salah");
	}
	else if(IsPlayerInRangeOfPoint(playerid, 25.0, 935.9503, 662.1276, 12.0029))  // untuk pompa bensin
	{
		if(GetPlayerJob(playerid) != JOB_TRUCKER || modelid != 514)
			return SendClientMessage(playerid, 0xCECECEFF, "Anda tidak bekerja sebagai distributor bahan bakar atau telah mengambil jalur yang salah");
	}
	else
		return SendClientMessage(playerid, 0xCECECEFF, "Anda harus berada di kilang");

	new total_litres = GetPlayerJobLoadItems(playerid);

	if(!IsPlayerInVehicle(playerid, vehicleid))
		return SendClientMessage(playerid, 0xCECECEFF, "Anda harus berada di kilang dengan kendaraan yang berfungsi");

	if(!strlen(params))
		return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /buyf [kuantitas dalam liter]");

	if(!IsTrailerAttachedToVehicle(vehicleid))
		return SendClientMessage(playerid, 0xCECECEFF, "Kendaraan Anda tidak memiliki tangki bahan bakar");

	extract params -> new load_fuel;

	if(!(1 <= load_fuel <= 8000))
		return SendClientMessage(playerid, 0xCECECEFF, "Anda dapat memuat dari 1 hingga 8000 liter bahan bakar");

	if(factory_fuels < load_fuel)
		return SendClientMessage(playerid, 0xCECECEFF, "Tidak banyak bahan bakar di kilang");

	if((total_litres + load_fuel) > 8000)
		return SendClientMessage(playerid, 0xCECECEFF, "Tangki tidak begitu pas");

	if(GetPlayerMoneyEx(playerid) < (load_fuel * 10))
		return SendClientMessage(playerid, 0xCECECEFF, "Tidak cukup uang untuk membeli begitu banyak bahan bakar");

	GivePlayerMoneyEx(playerid, -(load_fuel * 10), "Pembelian bahan bakar (kilang minyak)", true, false);

	SetRepositoryData(REPOSITORY_TYPE_OIL_FACTORY, REPOSITORY_ACTION_OIL_FACTORY_F, R_AMOUNT, factory_fuels - load_fuel);
	UpdateRepository(REPOSITORY_TYPE_OIL_FACTORY, REPOSITORY_ACTION_OIL_FACTORY_F);

	SetPlayerJobLoadItems(playerid, total_litres + load_fuel);

	new fmt_str[64];

	format(fmt_str, sizeof fmt_str, "Anda mengunggah %d liter total biaya bahan bakar $%d", load_fuel, load_fuel * 10);
	SendClientMessage(playerid, 0xFFFF00FF, fmt_str);

	format(fmt_str, sizeof fmt_str, "~g~+ %d litres~n~~b~total %d litres", load_fuel, total_litres + load_fuel);
	GameTextForPlayer(playerid, fmt_str, 2000, 6);

	format(fmt_str, sizeof fmt_str, "Pengiriman bahan bakar\n{FFFFFF}Memuat %d / 8000 l", total_litres + load_fuel);
	UpdateVehicleLabel(vehicleid, 0xCECECEFF, fmt_str);

	return 1;
}

CMD:sellf(playerid, params[])
{
	new modelid;
	new vehicleid = GetPlayerJobCar(playerid);

	if(vehicleid != INVALID_VEHICLE_ID)
		modelid = GetVehicleData(vehicleid, V_MODELID);

	if(GetPlayerTempJob(playerid) != TEMP_JOB_FACTORY_TRUCKER || modelid != 514)
		return SendClientMessage(playerid, 0xCECECEFF, "Anda tidak bekerja di layanan pengiriman pabrik");

	if(!IsPlayerInRangeOfPoint(playerid, 10.0, -1111.9989, 2169.4163, 38.0353))
		return SendClientMessage(playerid, 0xCECECEFF, "Anda harus berada di gudang bahan bakar pabrik");

	new total_litres = GetPlayerJobLoadItems(playerid);

	new factory_fuels = GetRepositoryData(REPOSITORY_TYPE_FACTORY, REPOSITORY_ACTION_FACTORY_FUEL, R_AMOUNT);

	if(!strlen(params))
		return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /sellf [kuantitas dalam liter]");

	extract params -> new sell_fuels;

	if(!(1 <= sell_fuels <= 8000))
		return SendClientMessage(playerid, 0xCECECEFF, "Anda dapat menjual dari 1 hingga 8000 liter bahan bakar");

	if(vehicleid == INVALID_VEHICLE_ID || !IsTrailerAttachedToVehicle(vehicleid) || total_litres < sell_fuels)
		return SendClientMessage(playerid, 0xCECECEFF, "Truk Anda tidak memiliki banyak bahan bakar atau tangki tidak terpasang");

	if((factory_fuels + sell_fuels) > 1000000)
		return SendClientMessage(playerid, 0xCECECEFF, "Depot bahan bakar pabrik penuh");

	GivePlayerMoneyEx(playerid, (sell_fuels * 12), "Penjualan bahan bakar (tanaman)", true, true);

	SetRepositoryData(REPOSITORY_TYPE_FACTORY, REPOSITORY_ACTION_FACTORY_FUEL, R_AMOUNT, factory_fuels + sell_fuels);
	UpdateRepository(REPOSITORY_TYPE_FACTORY, REPOSITORY_ACTION_FACTORY_FUEL);

	SetPlayerJobLoadItems(playerid, total_litres - sell_fuels);
	AddPlayerData(playerid, P_JOB_WAGE, +, sell_fuels * 2);

	new fmt_str[64];

	format(fmt_str, sizeof fmt_str, "Anda dijual ke pabrik %d liter bahan bakar dalam jumlah $%d", sell_fuels, sell_fuels * 12);
	SendClientMessage(playerid, 0x32a44bFF, fmt_str);

	format(fmt_str, sizeof fmt_str, "Penghasilan bersih Anda adalah {FF9900}$%d", sell_fuels * 2);
	SendClientMessage(playerid, 0x32a44bFF, fmt_str);

	format(fmt_str, sizeof fmt_str, "Pengiriman bahan bakar\n{FFFFFF}Memuat %d / 8000 l", total_litres - sell_fuels);
	UpdateVehicleLabel(vehicleid, 0xCECECEFF, fmt_str);

	return 1;
}

CMD:buyprod(playerid, params[])
{
	if(!(GetPlayerJob(playerid) == JOB_TRUCKER && IsPlayerInJob(playerid)))
		return SendClientMessage(playerid, 0xCECECEFF, "Anda harus bekerja sebagai sopir truk");

	extract params -> new buy_prod_count; else return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /buyprod [jumlah]. Biaya 1 produk 24$");

	if(!IsPlayerInRangeOfPoint(playerid, 20.0, 1238.0892,177.7890,20.3305))
		return SendClientMessage(playerid, 0xCECECEFF, "Anda harus berada di gudang sumber pabrik");

	else if(!IsPlayerInVehicle(playerid, GetPlayerJobCar(playerid)))
		return SendClientMessage(playerid, 0xCECECEFF, "Anda harus berada di gudang bahan sumber pabrik dalam transportasi yang berfungsi");

	else if(!(1 <= buy_prod_count <= 10000))
		return SendClientMessage(playerid, 0xCECECEFF, "Anda dapat memuat dari 1 hingga 10.000 PC. produk");

	else if(GetRepositoryData(REPOSITORY_TYPE_FACTORY, REPOSITORY_ACTION_FACTORY_PROD, R_AMOUNT) < buy_prod_count)
		return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada begitu banyak produk dalam persediaan");

	else if((buy_prod_count + GetPlayerJobLoadItems(playerid)) > 10000)
		return SendClientMessage(playerid, 0xCECECEFF, "Van tidak muat begitu banyak, maksimal 10.000 buah.");

	if(GetPlayerMoneyEx(playerid) < (buy_prod_count * 24))
		return SendClientMessage(playerid, 0xCECECEFF, "Tidak cukup uang untuk membeli begitu banyak produk");

	GivePlayerMoneyEx(playerid, -(buy_prod_count * 24), "Pembelian produk (produk yang dikembangkan)", true, true);

	SetPlayerJobLoadItems(playerid, GetPlayerJobLoadItems(playerid) + buy_prod_count);

	SetRepositoryData(REPOSITORY_TYPE_FACTORY, REPOSITORY_ACTION_FACTORY_PROD, R_AMOUNT, GetRepositoryData(REPOSITORY_TYPE_FACTORY, REPOSITORY_ACTION_FACTORY_PROD, R_AMOUNT) - buy_prod_count);
	UpdateRepository(REPOSITORY_TYPE_FACTORY, REPOSITORY_ACTION_FACTORY_PROD);

	new fmt_msg[64];

	format(fmt_msg, sizeof fmt_msg, "Kamu membeli %d produk bernilai $%d", buy_prod_count, buy_prod_count * 24);
	SendClientMessage(playerid, 0x32a44bFF, fmt_msg);

	format(fmt_msg, sizeof fmt_msg, "Jumlah produk yang diunduh saat ini: %d из 10000", GetPlayerJobLoadItems(playerid));
	SendClientMessage(playerid, 0xFFFF00FF, fmt_msg);

	return 1;
}

CMD:buym(playerid, params[])
