CMD:setstats(playerid, params[])
{
	if(GetPlayerAdminEx(playerid) < 5) return 1;

	extract params -> new to_player, set_code, set_value; else {

		SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /setstats [id pemain] [kode] [nilai]");
		SendClientMessage(playerid, 0xCECECEFF, "Kode: 1 - Level | 2 - Bekerja");

		return 1;
	}

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player)) return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");

	new fmt_msg[128];

	switch(set_code)
	{
		case 1:
		{
			if(!(1 <= set_value <= 25)) return SendClientMessage(playerid, 0xCECECEFF, "Tingkat 1 hingga 25");

			SetPlayerData(to_player, P_LEVEL, set_value);

			SetPlayerScore(to_player, set_value);

			format(fmt_msg, sizeof fmt_msg, "%s %s mengubah level Anda menjadi %d", admins_rank_name[GetPlayerAdminEx(playerid)], GetPlayerNameEx(playerid), set_value);
			SendClientMessage(to_player, 0x1E90FFFF, fmt_msg);

			format(fmt_msg, sizeof fmt_msg, "Anda menipu pemain %s tingkat pada %d", GetPlayerNameEx(to_player), set_value);
			SendClientMessage(playerid, 0x32a44bFF, fmt_msg);
		}
		case 2:
		{
			if(!(0 <= set_value <= 6)) return SendClientMessage(playerid, 0xCECECEFF, "Id kantor dari 0 hingga 6");
			SetPlayerData(to_player, P_JOB, set_value);

			format(fmt_msg, sizeof fmt_msg, "%s mengatur Anda %s", GetPlayerNameEx(playerid), GetPlayerJobAndRankName(to_player));
			SendClientMessage(to_player, 0x1E90FFFF, fmt_msg);

			format(fmt_msg, sizeof fmt_msg, "Anda mengatur pemain %s bekerja %s", GetPlayerNameEx(to_player), GetPlayerJobAndRankName(to_player));
			SendClientMessage(playerid, 0x32a44bFF, fmt_msg);
		}
		default: return SendClientMessage(playerid, 0xCECECEFF, "Kesalahan: Kode yang dimasukkan salah");
	}

	mysql_format(mysql, fmt_msg, sizeof fmt_msg, "UPDATE accounts SET score=%d,job=%d WHERE id=%d", GetPlayerLevel(to_player), GetPlayerJob(to_player), GetPlayerAccountID(to_player));
	mysql_query(mysql, fmt_msg, false);

	return 1;
}

CMD:pay(playerid, params[])
{
	extract params -> new to_player, cash; else return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /pay [id pemain] [jumlahnya]");

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player))
		return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");

	if(!IsPlayerInRangeOfPlayer(playerid, to_player, 6.0))
		return SendClientMessage(playerid, 0xCECECEFF, "Pemain itu jauh dari Anda");

	if(GetPlayerLevel(playerid) < 3)
	    return SendClientMessage(playerid, 0xCECECEFF, "Anda dapat mentransfer uang hanya dari level 3");

	new fmt_text[100],
		max_money = GetPlayerLevel(playerid) > 25 ? 50000 : 10000;
	if(to_player == playerid) return SendClientMessage(playerid, 0xCECECEFF, "Transfer uang ke diri sendiri?");
	if(!(1 <= cash <= max_money))
	{
		format(fmt_text, sizeof fmt_text, "Jumlah harus dari 1 hingga $%d", max_money);

		SendClientMessage(playerid, 0xCECECEFF, fmt_text);
		return 1;
	}

	if(GetPlayerMoneyEx(playerid) >= cash)
	{
		format(fmt_text, sizeof fmt_text, "+%d", cash);
		SetPlayerChatBubble(to_player, fmt_text, 0x32a44bFF, 10.0, 5000);

		format(fmt_text, sizeof fmt_text, "-%d", cash);
		SetPlayerChatBubble(playerid, fmt_text, 0xCECECEFF, 10.0, 5000);

		format(fmt_text, sizeof fmt_text, "Mentransfer uang dari %s (id: %d)", GetPlayerNameEx(playerid), GetPlayerAccountID(playerid));
		GivePlayerMoneyEx(to_player, cash, fmt_text, true, true);

		format(fmt_text, sizeof fmt_text, "Mentransfer uang ke pemain %s (id: %d)", GetPlayerNameEx(to_player), GetPlayerAccountID(to_player));
		GivePlayerMoneyEx(playerid, -cash, fmt_text, true, true);

		format(fmt_text, sizeof fmt_text, "%s memberimu uang $%d", GetPlayerNameEx(playerid), cash);
		SendClientMessage(to_player, 0x1E90FFFF, fmt_text);

		format(fmt_text, sizeof fmt_text, "Anda lulus ke pemain %s uang $%d", GetPlayerNameEx(to_player), cash);
		SendClientMessage(playerid, 0x1E90FFFF, fmt_text);

		Action(playerid, "mengeluarkan dompetnya dan menyerahkan uang itu", _, false);

		if(g_admin_pay_log)
		{
		    format(fmt_text, sizeof fmt_text, "[A] %s[%d] diserahkan %s[%d] uang $%d", GetPlayerNameEx(playerid), playerid, GetPlayerNameEx(to_player), playerid, cash);
			SendMessageToAdmins(fmt_text, 0x66CC33FF);
		}
	}
	else SendClientMessage(playerid, 0xCECECEFF, "Anda tidak memiliki dana yang cukup.");

	return 1;
}

