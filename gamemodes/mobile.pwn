public: StartPlayerJob(playerid, jobid)
{
	if(GetPlayerJob(playerid) == jobid)
	{
		if(!IsPlayerInJob(playerid))
		{
			new job_car = GetPlayerJobCar(playerid);
			if(IsPlayerInVehicle(playerid, job_car))
			{
				new action_type = GetVehicleData(job_car, V_ACTION_TYPE);
				new car_type = (action_type - VEHICLE_ACTION_TYPE_BUS_DRIVER) + 1;

				if(jobid == car_type)
				{
					SetPlayerJobLoadItems(playerid, 0);

					SetPlayerData(playerid, P_JOB_WAGE, 0);
					SetPlayerData(playerid, P_IN_JOB, true);
					SetVehicleData(job_car, V_ACTION_ID, true);

					new fmt_str[128];

					switch(jobid)
					{
						case JOB_BUS_DRIVER:
						{
							new route_id = GetPlayerData(playerid, P_BUS_ROUTE);

							format(fmt_str, sizeof fmt_str, "%s\n{FFFFFF}Tarif: {FF9900}$%d", g_bus_routes[route_id][BR_NAME], GetPlayerData(playerid, P_JOB_TARIFF));
							CreateVehicleLabel(job_car, fmt_str, g_bus_routes[route_id][BR_COLOR], 0.0, 0.0, 2.6, 45.0);

							format(fmt_str, sizeof fmt_str, "mulai sopir bus bekerja di rute %s", g_bus_routes[route_id][BR_NAME]);
							Action(playerid, fmt_str, _, false);

							NextBusRouteCP(playerid);

							SetPVarFloat(playerid, "car_damage", 1000.0);
						}
						case JOB_TAXI_DRIVER:
						{
							format(fmt_str, sizeof fmt_str, "%s{FFFF00}Tarif: $%d", GetPlayerData(playerid, P_JOB_SERVICE_NAME), GetPlayerData(playerid, P_JOB_TARIFF));
							CreateVehicleLabel(job_car, fmt_str, 0x1E90FFFF, 0.0, 0.0, 1.4, 25.0);

							Action(playerid, "mulai bekerja sebagai sopir taksi", _, false);
						}
						case JOB_MECHANIC:
						{
							SetPlayerData(playerid, P_MECHANIC_FILL_PAY, 0);
							SetPlayerData(playerid, P_MECHANIC_REPAIR_PAY, 0);

							format(fmt_str, sizeof fmt_str, "%s{FFFFFF}Mekanik otomatis\n{CECECE}Tanpa bahan bakar", GetPlayerData(playerid, P_JOB_SERVICE_NAME));
							CreateVehicleLabel(job_car, fmt_str, 0xCC9900FF, 0.0, 0.0, 2.0, 25.0);

							Action(playerid, "montir otomatis mulai bekerja", _, false);
						}
						case JOB_TRUCKER:
						{
							if(!GetTruckIndex(job_car))
							{
								fmt_str = "Layanan\n{FFFFFF}Pengiriman produk";

								SendClientMessage(playerid, 0x32a44bFF, "Gunakan {1E90FF}/bizlist {32a44b}untuk melihat pesanan dari pemilik bisnis");
								Action(playerid, "mulai bekerja", _, false);
							}
							else
							{
								fmt_str = "Layanan\n{FFFFFF}Pengiriman bahan bakar";

								SendClientMessage(playerid, 0x32a44bFF, "Gunakan {1E90FF}/fuellist {32a44b}untuk melihat pesanan dari pemilik pompa bensin");
								Action(playerid, "truk pengiriman bahan bakar dimulai", _, false);
							}

							CreateVehicleLabel(job_car, fmt_str, 0x1E90FFFF, 0.0, 2.1, 2.1, 20.0);
						}
					}
				}
			}
		}
	}
	return 1;
}

public: EndPlayerJob(playerid)
{
    SetPlayerData(playerid, P_IN_JOB, false),SetPlayerTempJob(playerid, TEMP_JOB_NONE);
	if(IsPlayerInJob(playerid))
	{
		new vehicleid = GetPlayerJobCar(playerid);

		if(GetVehicleData(vehicleid, V_ACTION_TYPE) == VEHICLE_ACTION_TYPE_TRUCK_TRAIL) return 1;

		if(vehicleid != INVALID_VEHICLE_ID)
		{
			SetVehicleToRespawn(vehicleid);
			SetPlayerData(playerid, P_JOB_CAR, INVALID_VEHICLE_ID);
		}
	}
	KillEndJobTimer(playerid);

	new fmt_str[128];
	new wage = GetPlayerJobWage(playerid);
	new items = GetPlayerJobLoadItems(playerid);

	SetPlayerData(playerid, P_JOB_WAGE, 0);
	SendClientMessage(playerid, 0xFFFF00FF, "Hari kerja sudah berakhir!");

	SetPlayerData(playerid, P_IN_JOB, false);
	if(GetPlayerTempJob(playerid) == TEMP_JOB_FACTORY_TRUCKER)
		{
			RemovePlayerAttachedObjects(playerid);
			SetPlayerSkinInit(playerid);

			format(fmt_str, sizeof fmt_str, "Anda telah berhasil menyelesaikan hari kerja Anda.");
			SendClientMessage(playerid, 0xFFFFFFFF, fmt_str);

			SetVehicleToRespawn(GetPlayerData(playerid, P_JOB_CAR));

			SetPlayerData(playerid, P_JOB_CAR , INVALID_VEHICLE_ID);

			SetPlayerJobLoadItems(playerid, 0);
			SetPlayerTempJob(playerid, TEMP_JOB_NONE);
		}
    if(GetPlayerTempJob(playerid) == TEMP_JOB_PIZZA)
		{
			new pay_sum = (items * GetTempJobInfo(TEMP_JOB_PIZZA, TJ_PAY_FOR_LOAD));

			pay_sum *= g_doubling[DOUBLING_WAGE];
		    DisablePlayerCheckpoint(playerid);
			format(fmt_str, sizeof fmt_str, "Hari kerja berakhir. Total terkirim: {FFFFFF}%d", GetPlayerJobLoadItems(playerid));
			SendClientMessage(playerid, 0xffff00FF, fmt_str);

			format(fmt_str, sizeof fmt_str, "Total yang diterima {FFFFFF}$%d", pay_sum);
			SendClientMessage(playerid, 0xffff00FF, fmt_str);

			GivePlayerMoneyEx(playerid, pay_sum, "Gaji petugas pengiriman pizza", true, false);

			format(fmt_str, sizeof fmt_str, "~b~~h~+$%d", pay_sum);
			GameTextForPlayer(playerid, fmt_str, 4000, 1);
			SetPlayerTempJobState(playerid, false);
			SetPlayerData(playerid, P_CHECK,0);
		}

	switch(GetPlayerData(playerid, P_JOB))
	{
		case JOB_BUS_DRIVER:
		{
			new Float: health = GetPVarFloat(playerid, "car_damage");
			new repair_sum;

			if(health < 1000.0)
				repair_sum = (floatround((1000.0 - health) + float(random(100)+10))) / 6;

			DisablePlayerRaceCheckpoint(playerid);

			format(fmt_str, sizeof fmt_str, "Berpenghasilan: {00FF00}$%d", wage + (items * GetPlayerData(playerid, P_JOB_TARIFF)));
			SendClientMessage(playerid, 0xFFFFFFFF, fmt_str);

			format(fmt_str, sizeof fmt_str, "Jumlah penumpang: {00FF00}%d{FFFFFF}. Tiket yang dibeli untuk {00FF00}$%d", items, items * GetPlayerData(playerid, P_JOB_TARIFF));
			SendClientMessage(playerid, 0xFFFFFFFF, fmt_str);

			format(fmt_str, sizeof fmt_str, "Untuk perbaikan bus: {CECECE}-$%d", repair_sum);
			SendClientMessage(playerid, 0xFFFFFFFF, fmt_str);

			if(repair_sum < wage)
				wage -= repair_sum;

			//GiveMoney(playerid, salary, "Gaji Pengemudi Minibus", true, true);
		}
		case JOB_TAXI_DRIVER:
		{
		    DestroyVehicleLabel(GetPlayerData(playerid, P_JOB_CAR));
			DisablePlayerRaceCheckpoint(playerid);
			wage = items * GetPlayerData(playerid, P_JOB_TARIFF);

			format(fmt_str, sizeof fmt_str, "Berpenghasilan: {00FF00}$%d", wage);
			SendClientMessage(playerid, 0xFFFFFFFF, fmt_str);

			format(fmt_str, sizeof fmt_str, "Jumlah penumpang: {00FF00}%d", items);
			SendClientMessage(playerid, 0xFFFFFFFF, fmt_str);

		}
		case JOB_MECHANIC:
		{
			DisablePlayerRaceCheckpoint(playerid);
			wage = GetPlayerData(playerid, P_MECHANIC_FILL_PAY) + GetPlayerData(playerid, P_MECHANIC_REPAIR_PAY);
            DestroyVehicleLabel(GetPlayerData(playerid, P_JOB_CAR));
			format(fmt_str, sizeof fmt_str, "Diterima untuk diperbaiki: {00FF00}$%d", GetPlayerData(playerid, P_MECHANIC_REPAIR_PAY));
			SendClientMessage(playerid, 0xFFFFFFFF, fmt_str);

			format(fmt_str, sizeof fmt_str, "Diterima untuk pengisian bahan bakar: {00FF00}$%d", GetPlayerData(playerid, P_MECHANIC_FILL_PAY));
			SendClientMessage(playerid, 0xFFFFFFFF, fmt_str);

			format(fmt_str, sizeof fmt_str, "Total yang diterima: {ffff00}$%d", wage);
			SendClientMessage(playerid, 0xFFFFFFFF, fmt_str);
		}
	}

	SendClientMessage(playerid, 0x32a44bFF, "Uang akan ditransfer ke akun Anda pada saat gaji.");

	TransferCoffersMoney(-wage, "Membayar karyawan", "", false);

	AddPlayerData(playerid, P_WAGE, +, wage);
	SetVehicleToRespawn(GetPlayerData(playerid, P_JOB_CAR));
	SetPlayerData(playerid, P_JOB_CAR, INVALID_VEHICLE_ID);

	format(fmt_str, sizeof fmt_str, "UPDATE accounts SET wage=%d WHERE id=%d LIMIT 1", GetPlayerData(playerid, P_WAGE), GetPlayerAccountID(playerid));
	mysql_query(mysql, fmt_str, false);

	return 1;
}

