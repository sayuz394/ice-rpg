CMD:getlog(playerid, params[])
{
	if(GetPlayerAdminEx(playerid) < 4) return 1;

	if(!strlen(params))
	{
		SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /getlog [nama pemain] [log id] [jumlah hari]");
		SendClientMessage(playerid, 0xCECECEFF, "Pengidentifikasi:");
		SendClientMessage(playerid, 0xCECECEFF, "1 - Obrolan admin");
		SendClientMessage(playerid, 0xCECECEFF, "2 - Tanggapan Pelaporan");
		SendClientMessage(playerid, 0xCECECEFF, "3 - Tindakan Admin");
		SendClientMessage(playerid, 0xCECECEFF, "4 - Penerbitan Hak Administratif");
		SendClientMessage(playerid, 0xCECECEFF, "5 - Penerbitan Hak Kepemimpinan");
		SendClientMessage(playerid, 0xCECECEFF, "6 - Pesan SMS");
		SendClientMessage(playerid, 0xCECECEFF, "7 - Pesan Obrolan");
		SendClientMessage(playerid, 0xCECECEFF, "8 - Pertanyaan/pengaduan ke laporan");
		SendClientMessage(playerid, 0xCECECEFF, "9 - Tindakan Pemimpin/wakil");
		SendClientMessage(playerid, 0xCECECEFF, "10 - Tindakan Kepala Administrator");

		return 1;
	}

	extract params -> new string:name[21], log_id, time_limit;

	if(!(LOG_TYPE_ADMIN_CHAT <= log_id <= LOG_TYPE_SUPERADMIN_ACTION))
		return SendClientMessage(playerid, 0xCECECEFF, "ID tidak valid");

	if(!(0 <= time_limit <= 30))
		return SendClientMessage(playerid, 0xCECECEFF, "Jumlah hari adalah dari 1 hingga 30. Gunakan 0 untuk menampilkan semua tindakan");

	if(!(3 <= strlen(name) <= 20))
		return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain dengan nama ini ditemukan.");

	new query[144];
	new Cache: result,
		rows,
		user_id,
		admin;

	mysql_format(mysql, query, sizeof query, "SELECT id, admin FROM accounts WHERE name='%e' LIMIT 1", name);
	result = mysql_query(mysql, query);

	rows = cache_num_rows();

	if(rows)
	{
		user_id = cache_get_row_int(0, 0);
		admin = cache_get_row_int(0, 1);
	}

	cache_delete(result);

	if(rows && user_id)
	{
		if(GetPlayerAdminEx(playerid) >= admin)
		{
			mysql_format(mysql, query, sizeof query, "SELECT * FROM action_log WHERE acc_id=%d AND type=%d", user_id, log_id);

			new fmt_text[35];

			if(time_limit)
			{
				format(fmt_text, sizeof fmt_text, " AND time >= %d", gettime() - (time_limit * 86400));
				strcat(query, fmt_text);
			}

			result = mysql_query(mysql, query, true);

			rows = cache_num_rows();

			if(rows)
			{
				new log_name[10][37] =
				{
					"Obrolan admin",
					"Tanggapan Pelaporan",
					"Tindakan Admin",
					"Penerbitan Hak Administratif",
					"Penerbitan Hak Kepemimpinan",
					"Pesan SMS",
					"Pesan Obrolan",
					"Pertanyaan/keluhan ke laporanz",
					"Tindakan Pemimpin/wakil",
					"Tindakan Kepala Administrator"
				};

				format(query, sizeof query, "Rekaman ditemukan oleh parameter \"%s\" untuk pemain %s", log_name[log_id - 1], name);

				if(time_limit)
				{
					format(fmt_text, sizeof fmt_text, " lebih dari yang terakhir %d hari.", time_limit);
					strcat(query, fmt_text);
				}

				SendClientMessage(playerid, 0x66CC33FF, query);

				for(new idx; idx < rows; idx ++)
				{
					new desc[144],
						time[6],
						uip[17];

					cache_get_field_content(idx, "description", desc);
					cache_get_field_content(idx, "uip", uip);

					timestamp_to_date(cache_get_field_content_int(idx, "time"), time[2], time[1], time[0], time[3], time[4], time[5]);

					format
					(
						query, sizeof query,
						"#%d | %02d.%02d.%d в %02d:%02d:%02d | %s | %s",
						cache_get_field_content_int(idx, "id"),
						time[0], time[1], time[2], time[3], time[4], time[5],
						uip, desc
					);

					SendClientMessage(playerid, 0xffff00FF, query);
				}

				format(query, sizeof query, "Jumlah total entri: %d", rows);
				SendClientMessage(playerid, 0x66CC33FF, query);
			}
			else SendClientMessage(playerid, 0xCECECEFF, "Tidak ada catatan yang ditemukan di database untuk parameter ini");

			cache_delete(result);
		}
		else SendClientMessage(playerid, 0xCECECEFF, "Anda tidak dapat melihat log administrator di atas peringkat Anda");
	}
	else SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain dengan nama ini ditemukan.");

	return 1;
}

CMD:lip(playerid, params[])
