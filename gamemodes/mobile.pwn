CMD:mute(playerid, params[])
{
	if(GetPlayerAdminEx(playerid) < 1) return 1;

	if(!strlen(params)) return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /mute [id pemain] [waktu dalam hitungan menit] [alasan (opsional)]");

	extract params -> new to_player, mute_time, string: reason[30];

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player))
		return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");

    if(GetPlayerAdminEx(to_player) > GetPlayerAdminEx(playerid))
		return SendClientMessage(playerid, 0xCECECEFF, "Tindakan ini tidak dapat dilakukan dengan administrator dengan peringkat lebih tinggi.");

	if(!(1 <= mute_time <= 360)) return SendClientMessage(playerid, 0xCECECEFF, "Времяminimal harus 1 dan tidak lebih dari 360 menit");
	if(GetPlayerData(to_player, P_MUTE) > 0) return SendClientMessage(playerid, 0xCECECEFF, "Pemain ini sudah memiliki mut");
	if(GetPlayerAdminEx(to_player) > GetPlayerAdminEx(playerid)) return SendClientMessage(playerid, 0xCECECEFF, "Anda tidak dapat membuat lelucon pada administrator dengan peringkat lebih tinggi");
	//else if(to_player == playerid) return SendClientMessage(playerid, 0xCECECEFF, "Anda tidak bisa pasang sendiri");

	new fmt_msg[128];
	format(fmt_msg, sizeof fmt_msg, "%s %s membuat lelucon bagi pemain %s pada %d min", admins_rank_name[GetPlayerAdminEx(playerid)], GetPlayerNameEx(playerid), GetPlayerNameEx(to_player), mute_time);

	if(strlen(reason))
		format(fmt_msg, sizeof fmt_msg, "%s. Alasan: %s", fmt_msg, reason);

	SendClientMessageToAll(0xFF5533FF, fmt_msg);

	SetPlayerData(to_player, P_MUTE, mute_time * 60);
	UpdatePlayerDatabaseInt(to_player, "mute", mute_time * 60);

	SendClientMessage(to_player, 0xCECECEFF, "Saatnya untuk mengakhiri larangan ngobrol: {CCCC00}/time");

	format(fmt_msg, sizeof fmt_msg, "Выдал %s[acc:%d] obrolan memblokir %d min Alasan: %s",
	GetPlayerNameEx(to_player), GetPlayerAccountID(to_player), mute_time, reason);

	SendLog(playerid, LOG_TYPE_ADMIN_ACTION, fmt_msg);

	return 1;
}

