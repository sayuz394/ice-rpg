		return SendClientMessage(playerid, 0xCECECEFF, "Anda harus berada di gudang tambang dalam transportasi yang berfungsi");

	if(!(1 <= buy_metal_count <= 500))
		return SendClientMessage(playerid, 0xCECECEFF, "Anda dapat memuat dari 1 hingga 500 kg logam");

	if(miner_metal < buy_metal_count)
		return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada banyak logam di gudang tambang");

