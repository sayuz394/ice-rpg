CMD:invite(playerid, params[])
{
	if(!(1 <= GetPlayerTeamEx(playerid) <= 10)) return SendClientMessage(playerid, 0xCECECEFF, "Fungsi ini tidak tersedia untuk Anda");
	if(!(9 <= GetPlayerJob(playerid) <= 10)) return SendClientMessage(playerid, 0xCECECEFF, "Fungsi ini tersedia untuk para pemimpin organisasi dan wakil mereka");
	if(!strlen(params)) return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /invite [id pemain]");

	extract params -> new to_player;
	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player) || to_player == playerid) return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");
	if(1 <= GetPlayerTeamEx(to_player) <= 10) return SendClientMessage(playerid, 0xCECECEFF, "Pemain ini sudah menjadi organisasi.");
	if(GetPlayerData(to_player, P_WARN)) return SendClientMessage(playerid, 0xCECECEFF, "Pemain memiliki peringatan.");

	if(!IsPlayerInRangeOfPlayer(playerid, to_player, 10.0)) return SendClientMessage(playerid, 0xCECECEFF, "Pemainnya terlalu jauh");

	SendPlayerOffer(playerid, to_player, OFFER_TYPE_INVITE);
	return 1;
}

CMD:uninvite(playerid, params[])
{
	if(!(1 <= GetPlayerTeamEx(playerid) <= 10)) return SendClientMessage(playerid, 0xCECECEFF, "Fungsi ini tidak tersedia untuk Anda");
	if(!(9 <= GetPlayerJob(playerid) <= 10)) return SendClientMessage(playerid, 0xCECECEFF, "Fungsi ini tersedia untuk para pemimpin organisasi dan wakil mereka");

	if(!strlen(params)) return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /uninvite [id pemain] [alasan]");

	extract params -> new to_player, string:reason[32];

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player) || to_player == playerid)
		return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");

	if(!(1 <= strlen(reason) <= 32))
		return SendClientMessage(playerid, 0xCECECEFF, "Masukkan alasan pemecatan");

	if(GetPlayerTeamEx(to_player) != GetPlayerTeamEx(playerid))
		return SendClientMessage(playerid, 0xCECECEFF, "Pemain ini bukan anggota organisasi Anda.");

	if(GetPlayerJob(to_player) >= GetPlayerJob(playerid))
		return SendClientMessage(playerid, 0xCECECEFF, "Anda tidak dapat memecat pemberhentian wakil/pemimpin organisasi");

	UnInvite(playerid, to_player, reason);

	return 1;
}

CMD:uninviteoff(playerid, params[])
{
	if(!(1 <= GetPlayerTeamEx(playerid) <= 10)) return SendClientMessage(playerid, 0xCECECEFF, "Fungsi ini tidak tersedia untuk Anda");
	if(!(9 <= GetPlayerJob(playerid) <= 10)) return SendClientMessage(playerid, 0xCECECEFF, "Fungsi ini tersedia untuk para pemimpin organisasi dan wakil mereka");
	if(!strlen(params)) return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /uninviteoff [nama panggilan pemain]");

	extract params -> new string: player_name[21];

	if(IsPlayerConnected(GetPlayerID(player_name))) return SendClientMessage(playerid, 0xCECECEFF, "Seorang pemain dengan nama ini ada di server");

	new query[144],
		Cache: result,
		rows,
		team,
		rank;

	mysql_format(mysql, query, sizeof query, "SELECT team, job FROM accounts WHERE name='%s'", player_name);
	result = mysql_query(mysql, query, true);

	rows = cache_num_rows();

	if(rows)
	{
		team = cache_get_row_int(0, 0);
		rank = cache_get_row_int(0, 1);
	}

	cache_delete(result);

	if(!rows) return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain dengan nama ini ditemukan.");

	if(team != GetPlayerTeamEx(playerid))
		return SendClientMessage(playerid, 0xCECECEFF, "Pemain ini bukan anggota organisasi Anda.");

	if(rank >= GetPlayerJob(playerid))
		return SendClientMessage(playerid, 0xCECECEFF, "Anda tidak dapat memecat pemberhentian wakil/pemimpin organisasi");

	format(query, sizeof query, "Anda memecat seorang pemain %s offline", player_name);
	SendClientMessage(playerid, 0xFF5533FF, query);

	format(query, sizeof query, "UPDATE accounts SET team=0,org_skin=0,job=0 WHERE name='%s'", player_name);
	mysql_query(mysql, query, false);

	return 1;
}
