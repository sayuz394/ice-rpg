CMD:funinvite(playerid, params[])
{
	if(!GetPlayerData(playerid, P_FAMILY)) return SendClientMessage(playerid, 0xCECECEFF, "Fungsi ini tidak tersedia untuk Anda");
	if(!(9 <= GetPlayerData(playerid, P_FAMILY_RANK) <= 10)) return SendClientMessage(playerid, 0xCECECEFF, "Fungsi ini tersedia untuk kurator keluarga dan wakilnya");
	if(!strlen(params)) return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /funinvite [id pemain] [alasan]");

	extract params -> new to_player, string:reason[32];

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player) || to_player == playerid)
		return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");

	if(!(1 <= strlen(reason) <= 32))
		return SendClientMessage(playerid, 0xCECECEFF, "Masukkan alasan pemecatan");

	if(GetPlayerData(to_player, P_FAMILY) != GetPlayerData(playerid, P_FAMILY))
		return SendClientMessage(playerid, 0xCECECEFF, "Pemain ini bukan anggota keluarga Anda.");

	if(GetPlayerData(to_player, P_FAMILY_RANK) >= GetPlayerData(playerid, P_FAMILY_RANK))
		return SendClientMessage(playerid, 0xCECECEFF, "Anda tidak dapat memecat pemberhentian wakil/pemimpin organisasi");

	FUnInvite(playerid, to_player, reason);

	return 1;
}

