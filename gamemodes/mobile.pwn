CMD:selld(playerid, params[])
{
    new drugs = GetPlayerData(playerid, P_DRUGS);
	if(sscanf(params,"udd", params[0], params[1], params[2]))
	return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /selldrugs [id pemain] [jumlah] [biaya]");
	if(params[0] == INVALID_PLAYER_ID || params[0] == playerid) return SendClientMessage(playerid, -1,"Pemain ini tidak ada.!");
	if(drugs < params[1]) return SendClientMessage(playerid, -1, "Anda tidak memiliki jumlah obat yang banyak!");
	if(params[1]  < 1 || params[1] > 50) return SendClientMessage(playerid, -1, "Tidak kurang dari satu dan lebih dari 50");
	if(params[2] < 1 || params[2] > 1000) return SendClientMessage(playerid, -1,"Harga harus lebih tinggi dari 1 dan tidak lebih dari 1000");
	if(GetPlayerData(params[0], P_DRUGS) >= 500) return SendClientMessage(playerid, -1, "Pemain tidak bisa membeli lagi!");
   	if(!IsPlayerInRangeOfPlayer(playerid, params[0], 6.0))
		return SendClientMessage(playerid, 0xCECECEFF, "Pemain itu jauh dari Anda");

	if(GetPlayerMoneyEx(params[0]) < params[2])
		return SendClientMessage(playerid, 0xCECECEFF, "Pemain ini tidak memiliki jumlah itu.");

	SendPlayerOffer(playerid, params[0], OFFER_TYPE_SELL_DRUGS, drugs, params[2]);
	return 1;
}
CMD:drugs(playerid, params[])
{
	if(!strlen(params)) return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /drugs [gram]");
	extract params -> new drugs;
	if(!(1 <= drugs <= 3)) return SendClientMessage(playerid, 0xCECECEFF, "Jumlah gram dari 1 hingga 3");
	if(GetPlayerData(playerid, P_USE_DRUGS) != 0)
	    return SendClientMessage(playerid,0xCECECEFF,"Anda dapat menggunakan obat ini setiap 60 detik sekali!");
	new fmt_msg[512];

	if(GetPlayerData(playerid, P_DRUGS) < drugs) return SendClientMessage(playerid, 0xCECECEFF, "Anda tidak punya banyak obat");

	AddPlayerData(playerid, P_DRUGS, -, drugs);

	new Float:health;
    ApplyAnimation(playerid,"SMOKING","M_smk_drag",4.1,0,0,0,0,0);
	GetPlayerHealth(playerid, health);
	SetPlayerHealthEx(playerid, health + (drugs*20.0));

	SetPlayerData(playerid, P_USE_DRUGS, 60);
	SetPlayerWeather(playerid, -68);

	SetTimerEx("DrugEffectGone", 60000, false, "i", playerid);

	Action(playerid, "obat bekas", _, false);

	format(fmt_msg, sizeof fmt_msg, "Kamu dulu %d gram(а) obat-obatan. Kesehatan Anda (%f)", drugs, GetPlayerData(playerid, P_HEALTH));
	SendClientMessage(playerid, 0x32a44bFF, fmt_msg);

	format(fmt_msg, sizeof fmt_msg, "UPDATE accounts SET drugs=%d WHERE id=%d", GetPlayerData(playerid, P_DRUGS), GetPlayerAccountID(playerid));
	mysql_query(mysql, fmt_msg, false);

	return 1;
}

