CMD:lock(playerid, params[])
{
	if(!strlen(params))
		return SendClientMessage(playerid, 0x999999FF, "Gunakan: /lock [jenis] (1-Transportasi pribadi 2-Mobil sewaan 3-Transportasi kerja 4-Transportasi Tepercaya)");

	extract params -> new type;

	new Float: radius = 20.0;
	new vehicleid = INVALID_VEHICLE_ID;

	switch(type)
	{
		case 1:
		{
			vehicleid = GetPlayerOwnableCar(playerid);

			if(vehicleid == INVALID_VEHICLE_ID)
			{
				if(GetPlayerOwnableCars(playerid) == 0)
					return SendClientMessage(playerid, 0x999999FF, "Anda tidak memiliki transportasi pribadi");
				else
					return SendClientMessage(playerid, 0x999999FF, "Transportasi pribadi Anda tidak diunggah ke server");
			}
		}
		case 2:
		{
			vehicleid = GetPlayerRentCar(playerid);

			if(vehicleid == INVALID_VEHICLE_ID)
				return SendClientMessage(playerid, 0x999999FF, "Anda tidak punya mobil sewaan");
		}
		case 3:
		{
			vehicleid = GetPlayerJobCar(playerid);

			if(vehicleid == INVALID_VEHICLE_ID)
				return SendClientMessage(playerid, 0x999999FF, "Anda tidak dapat menggunakan fungsi ini.");
		}
		case 4:
		{
			vehicleid = GetPlayerAllowedCar(playerid);

			if(vehicleid == INVALID_VEHICLE_ID)
				return SendClientMessage(playerid, 0x999999FF, "Anda tidak memiliki transportasi tepercaya");
		}
		default:
			return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /lock [jenis] (1-Transportasi pribadi 2-Mobil sewaan 3-Transportasi kerja 4-Transportasi Tepercaya)");
	}

	new Float: x, Float: y, Float: z;
	GetVehiclePos(vehicleid, x, y, z);

	if(IsPlayerInRangeOfPoint(playerid, radius, x, y, z))
	{
		new status = GetVehicleParam(vehicleid, V_LOCK);
		if(status)
		{
			if(type == 1)
			{
				Action(playerid, "membuka transportasi pribadi", _, true);
			}
			GameTextForPlayer(playerid,"~w~~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~CAR~g~ OPENED", 3000, 3);
			SetVehicleParam(vehicleid, V_LOCK, false);
		}
		else
		{
			if(type == 1)
			{
				Action(playerid, "transportasi pribadi tertutup", _, true);
			}
			GameTextForPlayer(playerid,"~w~~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~CAR~r~ CLOSED", 3000, 3);
			SetVehicleParam(vehicleid, V_LOCK, true);
		}
	}
	else SendClientMessage(playerid, 0x999999FF, "Anda harus berdiri di sebelah transportasi");

	return 1;
}


