CMD:skill(playerid, params[])
{
	extract params -> new to_player; else return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /skill [id pemain]");

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player))
		return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");

	if(to_player == playerid)
	{
		ShowPlayerSkills(playerid);
		return Action(playerid, "melihat melalui keterampilan senjatanya", _, false);
	}

