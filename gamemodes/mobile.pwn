		admin,
		day_prev,
		day_last,
		name[21],
		list[sizeof query * 25 + 60] = "Nama depan\t\t\tNilai\tDalam game kemarin\tDalam game hari ini\tStatus\n{FFFFFF}";

	mysql_format(mysql, query, sizeof query, "SELECT * FROM accounts WHERE admin > 0 ORDER BY admin DESC");
