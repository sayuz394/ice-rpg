		    SendMessageToTeam(TEAM_HOSPITAL,l_string,0x1E90FFFF);
		}
	}
	format(l_string,sizeof(l_string),"{FFFF00}[Tantangan]{FFFFFF} %s %s menerima panggilan Anda. Harapkan!",GetPlayerJobAndRankName(playerid),GetPlayerNameEx(playerid));
