CMD:kick(playerid, params[])
{
	if(GetPlayerAdminEx(playerid) < 2) return 1;

	if(!strlen(params)) return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /kick [id pemain] [alasan (opsional)]");

	extract params -> new to_player;

	if(!IsPlayerConnected(to_player)) return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");
	else if(GetPlayerAdminEx(to_player) > GetPlayerAdminEx(playerid)) return SendClientMessage(playerid, 0xCECECEFF, "Anda tidak dapat menendang administrator di peringkat yang lebih tinggi");

	new reason[31];
	sscanf(params, "{d}s[30]", reason);

	new fmt_msg[128];
	format(fmt_msg, sizeof fmt_msg, "%s %s menendang pemain %s", admins_rank_name[GetPlayerAdminEx(playerid)], GetPlayerNameEx(playerid), GetPlayerNameEx(to_player));

	if(strlen(reason) > 0)
		format(fmt_msg, sizeof fmt_msg, "%s. Alasan: %s", fmt_msg, reason);

	SendClientMessageToAll(0xFF5533FF, fmt_msg);

	format(fmt_msg, sizeof fmt_msg, "Menendang %s[acc:%d]", GetPlayerNameEx(to_player), GetPlayerAccountID(to_player));
	SendLog(playerid, LOG_TYPE_ADMIN_ACTION, fmt_msg);

	Kick(to_player);

	return 1;
}

CMD:skick(playerid, params[])
{
	if(GetPlayerAdminEx(playerid) < 3) return 1;

	extract params -> new to_player; else return SendClientMessage(playerid, 0xCECECEFF, "Gunakan: /skick [id pemain]");

	if(!IsPlayerConnected(to_player)) return SendClientMessage(playerid, 0xCECECEFF, "Tidak ada pemain seperti itu");
	else if(GetPlayerAdminEx(to_player) > GetPlayerAdminEx(playerid)) return SendClientMessage(playerid, 0xCECECEFF, "Anda tidak dapat menendang administrator di peringkat yang lebih tinggi");

	new fmt_msg[128];

