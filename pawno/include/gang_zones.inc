stock CreateGangZonesCR()
{
	new const 
		colors[] = {0xFFFF0090, 0x3399FF90, 0x66CC0090, 0x99999990};
	
	for(new idx; idx < sizeof g_cr_zones_data; idx ++)
	{
		g_cr_zone_id[idx] = GangZoneCreate(g_cr_zones_data[idx][CRZ_MIN_X], g_cr_zones_data[idx][CRZ_MIN_Y], g_cr_zones_data[idx][CRZ_MAX_X], g_cr_zones_data[idx][CRZ_MAX_Y]);
		g_cr_zones_data[idx][CRZ_COLOR] = colors[random(sizeof colors)];
	}
}

stock DestroyGangZonesCR()
{
	for(new idx; idx < sizeof g_cr_zones_data; idx ++)
	{
		GangZoneDestroy(g_cr_zone_id[idx]);
		g_cr_zone_id[idx] = 0;
	}
}

stock ShowForPlayerGangZonesCR(playerid)
{
	for(new idx; idx < sizeof g_cr_zones_data; idx ++)
	{
		GangZoneShowForPlayer(playerid, g_cr_zone_id[idx], g_cr_zones_data[idx][CRZ_COLOR]);
	}
}